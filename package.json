{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ENV=dev tsc && node ./dist/server.js",
    "watch": "ENV=dev tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "ENV=test db-migrate --env test down -c5 && db-migrate --env test up && tsc && jasmine-ts",
    "migrate-up": "ENV=dev db-migrate up --config ./database.json",
    "migrate-down": "ENV=dev db-migrate down -c5 --config ./database.json",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/pg": "^7.14.7",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "concurrently": "^7.2.2",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "morgan": "^1.10.0",
    "prettier": "^2.7.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
